{
	"name": "Webcirque WebMP 1.0",
	"short_name": "WebMP 1.0",
	"version": "1.0.7.11",
	"manifest_version": 2,
	"description": "__MSG_extDescription__",
	/*"icons": {
		"16": "img/icon.png",
		"32": "img/icon32.png",
		"48": "img/icon64.png",
		"64": "img/icon64.png",
		"128": "img/icon128.png"
	},*/
	"default_locale": "en",
	"browser_action": {
		"default_icon": "img/placeholder.png",
		"default_title": "WebMP 1.0",
		"default_popup": "popup.htm"
	},
	"homepage_url": "https://webcirque.github.io/pwcq.dev/projects/webmp",
	"permissions": [
		"geolocation", "http://*/*", "https://*/*", "http://localhost/*", "file:///*", "notifications", "activeTab", "tabs", "webRequest", "webRequestBlocking"
	],
	"optional_permissions": [
		"cookies", "tabCapture", "desktopCapture", "downloads"
	],
	"incognito": "split"
}
